{
    "name": "Jira Server comment button upper",
    "description": "Google Chrome plugin aimed to move up the comment button at issue's page.",
    "version": "0.1",
    "author": "n.voronchev@gmail.com",
    "homepage_url": "https://github.com/NVoronchev/jira_server_comment_button_upper",
    "permissions": [
        "scripting",
        "storage"
    ],
    "host_permissions": ["<all_urls>"],
    "background": {
        "service_worker": "background.js"
    },
    "options_ui": {
        "page": "options.html",
        "open_in_tab": true
    },
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "96": "icon96.png",
        "128": "icon128.png"
      },
    "manifest_version": 3
}
